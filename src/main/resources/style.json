{
  "style": [
    {
      "selector": "edge:selected",
      "style": {
        "line-style": "dashed"
      }
    },
    {
      "selector": "edge",
      "style": {
        "label": "data(name)",
        "font-size": "10px",
        "text-margin-y": "-10px",
        "text-margin-x": "0px",
        "curve-style": "bezier",
        "arrow-scale": 0.8,
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "node:selected",
      "style": {
        "border-width": "6px",
        "border-color": "rgb(170,216,255)",
        "border-opacity": "0.5",
        "background-color": "rgb(119,130,140)"
      }
    },
    {
      "selector": "node[grouped = \"true\"]",
      "style": {
        "label": "data(group_name)",
        "border-width": "3px",
        "border-color": "rgb(66,170,255)",
        "border-opacity": "1"
      }
    },
    {
      "selector": "node:locked",
      "style": {
        "border-width": "6px",
        "border-color": "rgb(128, 128, 128)",
        "border-opacity": "0.5",
        "background-color": "rgb(119,130,140)"
      }
    },
    {
      "selector": "node",
      "style": {
        "label": "data(name)",
        "font-size": "10px"
      }
    },

    {
      "selector": "node[node_type = \"source\"]",
      "style": {
        "shape": "triangle",
        "height": "40px",
        "width": "40px"
      }
    },
    {
      "selector": "node[node_type = \"connector\"]",
      "style": {
        "shape": "rectangle",
        "height": "20px",
        "width": "20px"
      }
    },
    {
      "selector": "node[node_type = \"consumer\"]",
      "style": {
        "shape": "ellipse",
        "height": "30px",
        "width": "30px"
      }
    },
    {
      "selector": "node[system_type = \"heat\"]",
      "style": {
        "background-color": "rgb(255,0,0)"
      }
    },
    {
      "selector": "edge[system_type = \"heat\"]",
      "style": {
        "line-color": "rgb(139,0,0)",
        "target-arrow-color": "rgb(139,0,0)"
      }
    },
    {
      "selector": "node[system_type = \"power\"]",
      "style": {
        "background-color": "rgb(0,128,0)"
      }
    },
    {
      "selector": "edge[system_type = \"power\"]",
      "style": {
        "line-color": "rgb(0,100,0)",
        "target-arrow-color": "rgb(0,100,0)"
      }
    },
    {
      "selector": "node[system_type = \"fuel\"]",
      "style": {
        "background-color": "rgb(255,69,0)"
      }
    },
    {
      "selector": "edge[system_type = \"fuel\"]",
      "style": {
        "line-color": "rgb(255,169,0)",
        "target-arrow-color": "rgb(255,169,0)"
      }
    },
    {
      "selector": "node[system_type = \"cold\"]",
      "style": {
        "background-color": "rgb(0,0,255)"
      }
    },
    {
      "selector": "edge[system_type = \"cold\"]",
      "style": {
        "line-color": "rgb(30,144,255)",
        "target-arrow-color": "rgb(30,144,255)"
      }
    },
    {
      "selector": "node[system_type = \"other\"]",
      "style": {
        "background-color": "rgb(0,0,0)"
      }
    },
    {
      "selector": "edge[system_type = \"other\"]",
      "style": {
        "line-color": "rgb(105,105,105)",
        "target-arrow-color": "rgb(105,105,105)"
      }
    }
  ],
  "data": {},
  "zoomingEnabled": true,
  "userZoomingEnabled": true,
  "zoom": 1,
  "minZoom": 1e-50,
  "maxZoom": 1e50,
  "panningEnabled": true,
  "userPanningEnabled": true,
  "pan": {
    "x": 0,
    "y": 0
  },
  "boxSelectionEnabled": true,
  "wheelSensitivity": 0.5,
  "renderer": {
    "name": "canvas"
  }
}
